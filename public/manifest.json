{
	"manifest_version": 2,
	"name": "Covid19 India Tracker",
	"description": "Extension for covid stats for India",
	"version": "0.1.0",
	"content_security_policy": "script-src 'self' 'sha256-EqVRGre3zprZChb6CozlVn8JDNVPmmDVwPJ6xRR44OU='; object-src 'self'",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "index.html"
	},
	"icons": { "16": "icon.png", "48": "icon.png", "128": "icon.png" },
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": ["storage", "alarms"]
}
